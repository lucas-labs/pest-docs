{
    "semi": true,
    "singleQuote": true,
    "printWidth": 94,
    "tabWidth": 4,
    "endOfLine": "auto",
    "overrides": [
        {
            "files": ["*.yml", "*.yaml"],
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": ["*.tsx"],
            "options": {
                "printWidth": 100
            }
        }
    ]
}
